{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "palette": {
        "pink": "#F6BDE6",
        "mauve": "#C6A0F6",
        "base": "#24273A",
        "blue": "#8AADF4",
        "green": "#A6DA95",
        "red": "#ED8796",
        "teal": "#8BD5CA",
        "flamingo": "#F0C6C6",
        "maroon": "#EE99A0",
        "peach": "#F5A97F",
        "sapphire": "#7DC4E4",
        "surface0": "#363A4F",
        "surface1": "#494D64",
        "text": "#CAD3F5",
        "mantle": "#1e2030"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "background": "p:green",
          "foreground": "p:mantle",
          "properties": {
            "display_host": false
          },
          "style": "diamond",
          "template": "{{if .Root}} \uf0e7 {{.UserName}} {{else}} {{.UserName}} {{end}}",
          "type": "session"
        },
        {
          "background": "p:blue",
          "foreground": "p:mantle",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "\uE0A0 ",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} \ue0b1 {{ .BranchStatus }} {{ end }}{{ if .Working.Changed }}\ue0b1  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}\ue0b1  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ue0b1  {{ .StashCount }} {{ end }}",
          "foreground": "p:blue",
          "background": "p:surface0",
          "type": "git",
          "style": "diamond"
        },
        {
          "background": "p:mantle",
          "foreground": "p:text`",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " $ ",
          "type": "text"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
